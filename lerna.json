{
  "useWorkspaces": true,
  "version": "independent",
  "publishConfig": {
    "access": "@danbileee"
  },
  "ignoreChanges": ["**/*.md", "package.json"],
  "command": {
    "version": {
      "allowBranch": "main",
      "message": "chore(release): npm publish [skip ci]"
    },
    "publish": {
      "graphType": "all"
    }
  }
}
